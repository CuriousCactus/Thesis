pages = {743--748},
pmid = {3126247},
title = {{Development of resistance in \textit{Pseudomonas aeruginosa} to imipenem, norfloxacin, and ciprofloxacin during therapy: proof provided by typing with a DNA probe}},
url = {https://www.ncbi.nlm.nih.gov/pubmed/3126247},
volume = {157},
