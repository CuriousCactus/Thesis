@article{Cobessi2005,
abstract = {Pyochelin is a siderophore and virulence factor common to Burkholderia cepacia and several Pseudomonas strains. We describe at 2.0{\AA} resolution the crystal structure of the pyochelin outer membrane receptor FptA bound to the iron-pyochelin isolated from Pseudomonas aeruginosa. One pyochelin molecule bound to iron is found in the protein structure, providing the first three-dimensional structure at the atomic level of this siderophore. The pyochelin molecule provides a tetra-dentate coordination of iron, while the remaining bi-dentate coordination is ensured by another molecule not specifically recognized by the protein. The overall structure of the pyochelin receptor is typical of the TonB-dependent transporter superfamily, which uses the proton motive force from the cytoplasmic membrane through the TonB–ExbB–ExbD energy transducing complex to transport ferric ions across the bacterial outer membrane: a transmembrane 22 $\beta$-stranded barrel occluded by a N-terminal domain that contains a mixed four-stranded $\beta$-sheet. The N-terminal TonB box is disordered in two crystal forms, and loop L8 is found to point towards the iron–pyochelin complex, suggesting that the receptor is in a transport-competent conformation.},
author = {Cobessi, David and Celia, Herve and Pattus, Franc},
doi = {10.1016/j.jmb.2005.08.004},
file = {:C\:/Users/Lois/Google Drive/PhD/Papers/Cobessi, Celia, Pattus - 2005 - Crystal structure at high resolution of ferric-pyochelin and its membrane receptor FptA from textit{Pseu.pdf:pdf}},
isbn = {0022-2836 (Print)\n0022-2836 (Linking)},
issn = {00222836},
journal = {Journal of Molecular Biology},
keywords = {fpta,iron,membrane protein,pseudomonas aeruginosa,pyochelin transport,receptor},
number = {4},
pages = {893--904},
pmid = {16139844},
title = {{Crystal structure at high resolution of ferric-pyochelin and its membrane receptor FptA from \textit{Pseudomonas aeruginosa}}},
volume = {352},
year = {2005}
}
