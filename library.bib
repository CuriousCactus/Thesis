title = {{The \textit{Vibrio fischeri}-\textit{Euprymna scolopes} light organ association: current ecological paradigms}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16349524 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC106330},
volume = {64},
year = {1998}
}
