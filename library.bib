doi = {10.1016/j.ejmech.2010.07.031},
file = {:C\:/Users/Lois/Google Drive/PhD/Papers/Wang, Wan, Zhou - 2010 - Synthesis of novel sulfanilamide-derived 1,2,3-triazoles and their evaluation for antibacterial and antifungal.pdf:pdf},
