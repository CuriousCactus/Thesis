journal = {Metallomics},
keywords = {Bacteria,Bacteria: isolation & purification,Fluorescent Dyes,Fluorescent Dyes: metabolism,Fungi,Fungi: isolation & purification,Iron,Iron: metabolism,Siderophores,Siderophores: chemistry,Siderophores: metabolism},
month = {aug},
