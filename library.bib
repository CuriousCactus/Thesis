keywords = {Acinetobacter,Acinetobacter: drug effects,Acinetobacter: genetics,Acinetobacter: physiology,Acyl-Butyrolactones,Acyl-Butyrolactones: metabolism,Agrobacterium tumefaciens,Agrobacterium tumefaciens: drug effects,Agrobacterium tumefaciens: genetics,Agrobacterium tumefaciens: physiology,Escherichia coli,Escherichia coli: drug effects,Escherichia coli: genetics,Escherichia coli: physiology,Gene Expression Regulation, Bacterial,Gene Expression Regulation, Bacterial: drug effect,Indoles,Indoles: metabolism,Protein Binding,Protein Folding,Quorum Sensing,Quorum Sensing: drug effects,Transcription Factors,Transcription Factors: metabolism},
month = {dec},
