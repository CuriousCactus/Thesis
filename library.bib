pages = {1184--1192},
pmid = {22635110},
publisher = {Nature Publishing Group},
title = {{Microwave-assisted preparation of the quorum-sensing molecule 2-heptyl-3-hydroxy-4(1\textit{H})-quinolone and structurally related analogs}},
