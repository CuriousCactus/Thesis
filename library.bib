isbn = {1572-8773 (Electronic)\r0966-0844 (Linking)},
issn = {09660844},
journal = {BioMetals},
keywords = {Antimicrobial,Chelation,Hydroxypyridinone,Iron,Redox,Siderophore,Trojan horse},
number = {5},
pages = {1023--1036},
pmid = {22855208},
title = {{Characterization of the aqueous iron(III) chelation chemistry of a potential Trojan Horse antimicrobial agent: chelate structure, stability and pH dependent speciation}},
