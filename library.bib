year = {2002}
}
@article{grev:5128,
abstract = {For comparison with experimentally obtained thermochemical data, zero-point vibrational energies (ZPVEs) are required to convert total electronic energies obtained from ab initio quantum mechanical studies into 0 K enthalpies. The currently accepted practice is to employ self-consistent-field (SCF) harmonic frequencies that have been scaled to reproduce experimentally observed fundamental frequencies. This procedure introduces systematic errors that result from a recognizable flaw in the method, namely that the correct ZPVE, G(O), is not one half the sum of the fundamental vibrational frequencies. Until better methods for accurately determining ZPVEs are presented, we recommend using different scaling factors for the determination of ZPVEs than those used to compare theoretically determined harmonic frequencies to observed fundamentals.},
author = {Grev, Roger S. and Janssen, Curtis L. and Schaefer, Henry F.},
doi = {10.1063/1.461680},
file = {:C\:/Users/Lois/Google Drive/PhD/Papers/Grev, Janssen, Schaefer - 1991 - Concerning zero-point vibrational energy corrections to electronic energies.pdf:pdf},
issn = {00219606},
journal = {The Journal of Chemical Physics},
keywords = {CORRECTIONS,VIBRATIONAL STATES,ZEROPOINT ENERGY},
number = {7},
pages = {5128},
publisher = {AIP},
title = {{Concerning zero-point vibrational energy corrections to electronic energies}},
url = {http://link.aip.org/link/?JCP/95/5128/1 http://link.aip.org/link/JCPSA6/v95/i7/p5128/s1&Agg=doi},
volume = {95},
year = {1991}
