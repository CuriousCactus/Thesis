issn = {10745521},
journal = {Chemistry \& Biology},
keywords = {CHEMBIOL,MICROBIOL},
month = {jan},
number = {1},
pages = {87--96},
pmid = {17254955},
title = {{The \textit{Pseudomonas aeruginosa} 4-quinolone signal molecules HHQ and PQS play multifunctional roles in quorum sensing and iron entrapment}},
