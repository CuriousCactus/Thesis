isbn = {1464-3391 (Electronic)\r0968-0896 (Linking)},
issn = {09680896},
journal = {Bioorganic \& Medicinal Chemistry},
keywords = {Gram-negative bacteria,LuxR-type receptors,N-Acylated l-homoserine lactones,Pseudomonas aeruginosa,Quorum sensing,Thiolactone AHL analogs},
number = {16},
