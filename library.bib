issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
month = {dec},
number = {24},
pages = {11115--11119},
pmid = {1763027},
title = {{The C-terminal region of the \textit{Vibrio fischeri} LuxR protein contains an inducer-independent \textit{lux} gene activating domain}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.88.24.11115},
